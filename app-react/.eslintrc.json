{
  "env": {
    "browser": true,
    "es2015": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "prettier" // Add the Prettier plugin at the end
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "react", "react-hooks", "prettier"], // Include the 'prettier' plugin
  "rules": {
    "prettier/prettier": "error"
  }
}
